# Supabase
SUPABASE_URL=https://xyzcompany.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_REPORTS_BUCKET=reports

# Paddle (pending enablement)
PADDLE_ENV=sandbox
PADDLE_CLIENT_TOKEN=your_paddle_client_token
PADDLE_SANDBOX_PRICE_ID=pri_01hxexample
PADDLE_PRICE_ID=pri_01hxexample_live
PADDLE_WEBHOOK_SECRET=whsec_example_secret

# Upstash Redis (rate limiting)
UPSTASH_REDIS_REST_URL=https://your-upstash-redis-url.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_upstash_rest_token
RATE_LIMIT_ALLOW_FAIL_OPEN=false

# Email (Resend)
RESEND_API_KEY=re_1234567890example
EMAIL_FROM=TraitHarbor <support@traitharbor.com>
RESEND_OVERRIDE_TO=dev@traitharbor.com

# PDF rendering (Browserless)
BROWSERLESS_WS_ENDPOINT=wss://production-sfo.browserless.io/?token=YOUR_TOKEN
BROWSERLESS_TOKEN=your_browserless_token
BROWSERLESS_HOST=production-sfo.browserless.io

# Analytics
NEXT_PUBLIC_PLAUSIBLE_DOMAIN=traitharbor.com
NEXT_PUBLIC_SITE_URL=https://trait-harbor.vercel.app

# App behavior
NEXT_PUBLIC_QUIZ_FIXTURE_MODE=1
ALLOW_WEBHOOK_TEST_BYPASS=0
GUEST_SESSION_SECRET=replace_with_long_random_secret
REPORT_ACCESS_TOKEN_PEPPER=replace_with_long_random_secret
# Report rendering
REPORT_TEMPLATE_VERSION=v1
REPORT_LOCAL_FALLBACK=1
CHROME_EXECUTABLE_PATH="/Applications/Google Chrome.app/Contents/MacOS/Google Chrome"

# Local webhook simulation
WEBHOOK_URL=http://localhost:3000/api/paddle/webhook
ORDER_ID=ord_01hxexample
EVENT_TYPE=payment_succeeded
PADDLE_TRANSACTION_ID=txn_01hxexample
